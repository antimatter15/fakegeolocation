{
  "name": "Fake Geolocation",
  "version": "1.0",
  "description": "Allow faking of HTML5 geolocation.",
  "permissions": [
    "geolocation",
  ],
  "background_page": "background.html",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["geofake.js"]
    }
  ]
}
